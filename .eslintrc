{
  "root": true,
  "extends": "airbnb-base",
  "env": {
    "commonjs": true,
    "mocha": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "linebreak-style": [0, "windows"],
    "no-unused-vars": 1,
    "no-console": 0,
    "no-tabs": 0,
    "indent": 0,
    "no-useless-catch": 0,
    "comma-dangle": 0,
    "arrow-body-style": 1,
    "consistent-return": 0
  }
}
